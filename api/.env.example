# FastAPI microservice configuration
FASTAPI_APP_NAME=TikTokIO Media API
FASTAPI_AUTH_KEY=replace-with-the-same-key-stored-in-site_settings

# Database credentials (should mirror includes/config.php)
FASTAPI_DB_HOST=localhost
FASTAPI_DB_PORT=3306
FASTAPI_DB_NAME=tiktokio.mobi
FASTAPI_DB_USER=tiktokio.mobi
FASTAPI_DB_PASSWORD=changeme

# Storage + housekeeping
FASTAPI_STORAGE_DIR=../uploads/api-cache
FASTAPI_DOWNLOAD_TTL_SECONDS=7200
FASTAPI_CLEANUP_FREQUENCY=1800
FASTAPI_CLEANUP_GRACE=10800

# Provider defaults
FASTAPI_DEFAULT_PROVIDER=ytdlp
FASTAPI_HTTP_USER_AGENT=Mozilla/5.0 (compatible; TikTokIO/1.0)
